
{% extends 'AppBundle::layout.html.twig' %}

{% block body %}
    <h1>Password change</h1>

    {{ form_start(form) }}

    {{ form_row(form.username) }}
    {{ form_row(form.plainPassword.first) }}
    {{ form_row(form.plainPassword.second) }}

    <button type="submit">Register!</button>
    {{ form_end(form) }}

{% endblock %}

{% block title %}
    Password change
{% endblock %}












