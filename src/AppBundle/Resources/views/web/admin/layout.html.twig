            </li>

            <li class="nav-item {% if app.request.attributes.get('_route') starts with 'admin_user' %} active {% endif %}">
                <a class="nav-link" href="{{ path('admin_users_list') }}">Users</a>
            </li>



            <li class="nav-item {% if app.request.attributes.get('_route') starts with 'security' %} active {% endif %}">
